# ============================================================
# CoFit Collective - Environment Variables Template
# Copy this file to .env and fill in your actual values
# ============================================================

# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Firebase - Android
FIREBASE_ANDROID_API_KEY=your_android_api_key
FIREBASE_ANDROID_APP_ID=your_android_app_id

# Firebase - iOS / macOS
FIREBASE_IOS_API_KEY=your_ios_api_key
FIREBASE_IOS_APP_ID=your_ios_app_id
FIREBASE_IOS_BUNDLE_ID=com.example.cofitCollective

# Firebase - Web / Windows
FIREBASE_WEB_API_KEY=your_web_api_key
FIREBASE_WEB_APP_ID=your_web_app_id
FIREBASE_WINDOWS_APP_ID=your_windows_app_id
FIREBASE_WEB_MEASUREMENT_ID=your_web_measurement_id
FIREBASE_WINDOWS_MEASUREMENT_ID=your_windows_measurement_id

# Firebase - Common
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app

# FCM Service Account
FCM_PRIVATE_KEY_ID=your_private_key_id
FCM_PRIVATE_KEY=your_private_key
FCM_CLIENT_EMAIL=your_service_account_email
FCM_CLIENT_ID=your_client_id
